{
  "name": "unused-component-detector",
  "displayName": "Component Pruner",
  "description": "Scan, detect and safely remove unused React components from your codebase. Boost performance by eliminating dead code.",
  "version": "1.0.3",
  "publisher": "amitkrg124",
  "icon": "icon.png",
  "author": {
    "name": "amitkrg124"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Other"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unused-component-detector.scan",
        "title": "Find Unused Components",
        "category": "Unused Component Detector"
      },
      {
        "command": "unused-component-detector.delete",
        "title": "Delete Component",
        "category": "Unused Component Detector"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "unused-component-detector.scan",
          "when": "true"
        },
        {
          "command": "unused-component-detector.delete",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "vscode:prepublish": "npm run build",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.74.0",
    "typescript": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "vsce": "^2.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amitkrg124/unused-component-detector.git"
  },
  "homepage": "https://github.com/amitkrg124/unused-component-detector",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "unused",
    "detector",
    "cleanup",
    "dependencies",
    "dead code",
    "pruner",
    "typescript",
    "jsx",
    "tsx",
    "code cleanup",
    "bundle size",
    "performance"
  ]
}

